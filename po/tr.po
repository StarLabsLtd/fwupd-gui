msgid "Failed to enable LVFS"
msgstr "LVFS etkinleştirilemedi"

#: ../src/gfu-main.c:933
msgid "Failed to find LVFS"
msgstr "LVFS bulumamadı"

#: ../src/gfu-main.c:938
msgid "Failed to download metadata for LVFS"
msgstr "LVFS için üstveri indirilemedi"

#: ../src/gfu-main.c:980
msgid "Failed to download metadata"
msgstr "Üstveri indirilemedi"

